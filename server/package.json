    "compression": "1.7.4",
    "concurrently": "^3.6.1",
    "cors": "2.8.5",
    "express": "^4.16.3",
    "helmet": "3.22.0",
    "mongoose": "^5.2.7",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
